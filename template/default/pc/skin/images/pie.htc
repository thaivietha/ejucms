<script>
	var countdown = 60;
	function settime(val) {
		if (countdown == 0) {
			val.style.backgroundColor = "";
			val.removeAttribute("disabled");
			val.value = "免费获取验证码";
			countdown = 60;
			return;
		} else {
			val.style.backgroundColor = "graytext";
			val.setAttribute("disabled", true);
			val.value = "重新发送(" + countdown + ")";
			countdown--;
		}
		setTimeout(function() {
			settime(val)
		}, 1000)
	}
	
	function getPhoneCode(val){
		var mobile = $("#regForm #username").val();		
		$.ajax({
			url : "http://www.afang.com/login/getphonecode.html",
			data : {'mobile':mobile},
			type : "post",
			dataType : "json",
			success : function(data, status) {
				if (data.status == 1) {
					settime(val);
				}else{
					alert(data.msg);
				}
			}
		});
	}
	
	function regDo(){
		mobile = $("#regForm #username").val();
		captcha_code = $("#regForm #captcha_code").val();
		$.ajax({
			url : "http://www.afang.com/login/regdo.html",
			data : {'mobile':mobile,'captcha_code':captcha_code},
			type : "post",
			dataType : "json",
			success : function(data, status) {
				if (data.status == 1) {
					alert("初始密码已以短信形式的发送到了您的手机上！");
					location.reload();
				}else{
					alert(data.msg);
				}
			}
		});
	}
	
	function loginDo(){
		mobile = $("#loginForm #username").val();
		dpassword = $("#loginForm #password").val();
		$.ajax({
			url : "http://www.afang.com/login/logindo.html",
			data : {'mobile':mobile,'password':dpassword,'forward':'login'},
			type : "post",
			dataType : "json",
			success : function(data, status) {
				if (data.status == 1) {					
					location.reload();
				}else{
					alert(data.msg);
				}
			}
		});
	}
	
</script>
