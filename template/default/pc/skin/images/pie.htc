			<script type="text/javascript">
				//tab
				function resetTabs(obj) {
					$(obj).parent().parent().next("div").find(".fenlei_active").hide();
					$(obj).parent().parent().find("a").removeClass("current");
		
				}
		
				function loadTab() {
		
					$(".fenlei_content > .fenlei_active").hide();
					$(".tabs").each(function() {
						$(this).find("li:first a").addClass("current");
					});
					$(".fenlei_content").each(function() {
						$(this).find("div.fenlei_active:first").fadeIn();
					});
					$(".tabs a").on("mouseover", function(e) {
						e.preventDefault();
						if ($(this).attr("class") == "current") {
							return;
						
						}else {
							resetTabs(this);
							$(this).addClass("current");
							$($(this).attr("name")).fadeIn();
						}
					});
				}//
		
						
			</script>
			<script>
				$(function() {
					loadTab();
				});
			</script>
